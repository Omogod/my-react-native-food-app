{
  "expo": {
    "name": "OmoGod Foods",
    "slug": "Omogod-Foods",
    "description": "OmoGod Foods is a food delivery app that allows you to order food from your favorite restaurants and have it delivered to your doorstep.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/omogod-foods.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/omogod-foods.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
